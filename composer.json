{
  "name": "littlerocket/job-queue-bundle",
  "description": "Allows to run and schedule Symfony console commands as background jobs.",
  "keywords": [
    "job",
    "queue",
    "background",
    "cron",
    "scheduled"
  ],
  "homepage": "https://jmsyst.com/bundles/JMSJobQueueBundle",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Johannes M. Schmitt",
      "email": "schmittjoh@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^4.0 || ^5.0 || ^7.0",
    "symfony/error-handler": "^4.0 || ^5.0 || ^6.0 || ^7.0",
    "doctrine/common": "^3.5",
    "symfony/process": "^7.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "*",
    "symfony/yaml": "*",
    "symfony/browser-kit": "*",
    "symfony/finder": "*",
    "symfony/css-selector": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/orm": "*",
    "symfony/twig-bundle": "*",
    "symfony/form": "*",
    "symfony/validator": "*",
    "phpunit/phpunit": "*",
    "symfony/intl": "*"
  },
  "suggest": {
    "symfony/twig-bundle": "Required when using the webinterface."
  },
  "autoload": {
    "psr-0": {
      "JMS\\JobQueueBundle": ""
    }
  },
  "target-dir": "JMS/JobQueueBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "platform": {
      "php": "8.3"
    }
  }
}
